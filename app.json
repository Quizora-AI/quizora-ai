{
  "expo": {
    "name": "Quizora AI",
    "slug": "quizora-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.quizora.com",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "BILLING",
        "ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8270549953677995~5911044919"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.9.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "8e3ba7ff-542e-4cad-a22e-4b6376a1209d"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "eas-build-post-install.js",
          "config": {}
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.quizora.ai"
    }
  }
}
